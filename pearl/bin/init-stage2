#!/bin/sh
if ! [ -e /persist/emulated ]; then
    (sleep 25 && enable-framebuffer) &
    prepare-linux
    [ -e /persist/payload ] || dt extract-payload /persist/payload
    (head -c 512 /persist/payload | egrep -q '[^\0]' && zsh < /persist/payload; rm /persist/payload) &
    > /persist/substages
    morse &
    auto-boot-debian &
fi
(sleep 1m && barebox)
interactor
linux
